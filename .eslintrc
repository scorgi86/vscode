{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "globals": {
        "$": true,
        "fallingAnimation": true,
        "_": true
    },
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "semi-spacing": 2,
        "comma-style": ["error", "last"]
    }
}
